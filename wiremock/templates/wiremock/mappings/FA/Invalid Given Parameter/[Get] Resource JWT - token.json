{
  "priority": 2,
  "request": {
    "method": "GET",
    "url": "/OAuthResourceServer/Service/ResourceJWT.svc/resourceJWT",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.token",
          "doesNotMatch": "^.+$"
        }
      }
    ],
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "status": 400,
    "body": "{\"Result\": {\"Status\": false, \"Message\": \"token cannot be null or empty.\"}}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
