{
  "priority": 2,
  "request": {
    "method": "POST",
    "url": "/api/1.1/users",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.mac_address",
          "doesNotMatch": "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$"
        }
      }
    ],
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "status": 404,
    "body": "{\"Status\": false, \"Data\": {\"Message\": \"INPUT_PARAMETERS_MISSING\", \"Id\": null}, \"ProcessId\": null, \"Code\": \"400\", \"Message\": \"mac_address cannot be null or empty.\"}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
