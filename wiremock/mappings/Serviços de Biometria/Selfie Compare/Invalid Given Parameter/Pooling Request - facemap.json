{
  "priority": 2,
  "request": {
    "method": "POST",
    "url": "/api/1.1/document/selfie-compare-base64",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.facemap",
          "doesNotMatch": "^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$"
        }
      }
    ],
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "status": 400,
    "body": "{\"status\": false, \"code\": \"400\", \"message\": \"facemap cannot be null or empty\", \"data\": {\"message\": \"BAD_REQUEST\", \"verified\": false, \"confidence\": null}}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
