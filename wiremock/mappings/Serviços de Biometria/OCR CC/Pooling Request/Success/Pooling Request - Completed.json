{
  "priority": 3,
  "request": {
    "method": "POST",
    "url": "/api/1.1/document/pooling",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.doctype",
          "matches": "^(passport|citizen card|identity card)$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.front",
          "matches": "^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.back",
          "matches": "^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.processId",
          "matches": "^(0[1-9]|[1-9][0-9])$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.type",
          "matches": "^pooling$"
        }
      }
    ],
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "transformers": ["response-template"],
    "status": 200,
    "body": "{\"status\": true, \"message\": \"OCR Completed\", \"data\": {\"process\": {{randomInt lower=1 upper=99}}, \"message\": \"COMPLETED\", \"fields\": {\"idNumber\": \"123456789\", \"expirationDate\": \"2030-12-31\", \"birthDate\": \"1990-01-01\", \"additionalField1\": \"value1\", \"additionalField2\": \"value2\"}}}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
