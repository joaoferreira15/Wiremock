{
  "request": {
    "urlPath": "/webhook",
    "method": "POST"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "text/plain"
    },
    "body": "User created successfully"
  },
  "serveEventListeners": [
    {
      "name": "webhook",
      "parameters": {
        "method": "POST",
        "url": "http://presentation-instance-service-local:8081/callback",
        "headers": {
          "content-type": "application/json"
        },
        "body": "{{jsonPath originalRequest.body '$.cidadao'}}",
        "delay": {
          "type": "uniform",
          "lower": 5000,
          "upper": 7000
        }
      }
    }
  ]
}
