{
  "priority": 3,
  "request": {
    "method": "GET",
    "url": "/api/1.1/users/find-by-address",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.mac_address",
          "matches": "^([0-4A-Ca-c]{2}[:-]){5}([0-4A-Ca-c]{2})$"
        }
      }
    ],
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "transformers": ["response-template"],
    "status": 200,
    "body": "{\"status\": true, \"data\": {\"id\": {{randomInt lower=1 upper=99}}, \"message\": \"Process found.\"}}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
