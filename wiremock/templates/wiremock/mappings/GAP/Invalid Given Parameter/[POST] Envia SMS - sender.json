          "doesNotMatch": "^.+$"
        }
      }
    ]
  },
  "response": {
    "status": 400,
    "body": "<soapenv:Envelope xmlns:soapenv='http://schemas.xmlsoap.org/soap/envelope/' xmlns:ns='http://www.themindelectric.com/package/com.mobicomp.mmh.sms.webservice.server/'><soapenv:Header/><soapenv:Body><ns:MultiSmsStatusWS><ns:Status>false</ns:Status><ns:Message>sender cannot be null or empty</ns:Message><ns:SentCount>0</ns:SentCount><ns:FailedCount>1</ns:FailedCount></ns:MultiSmsStatusWS></soapenv:Body></soapenv:Envelope>",
    "headers": {
      "Content-Type": "text/xml; charset=utf-8"
    }
  }
}
