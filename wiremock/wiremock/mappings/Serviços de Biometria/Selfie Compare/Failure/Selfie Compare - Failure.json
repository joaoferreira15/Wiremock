{
  "priority": 3,
  "request": {
    "method": "POST",
    "url": "/api/1.1/document/selfie-compare-base64",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.photo1",
          "matches": "^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.photo2",
          "matches": "^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.photo",
          "matches": "^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.facemap",
          "matches": "^([A-Za-z0-9+/]{4})*([A-Za-z0-9+/]{3}=|[A-Za-z0-9+/]{2}==)?$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.livenessId",
          "matches": "^(399|3[0-9]{2})$"
        }
      }
    ],
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "transformers": ["response-template"],
    "status": 400,
    "body": "{\"status\": false, \"code\": \"400\", \"message\": \"Selfie comparison failed\", \"data\": {\"message\": \"FACE_COMPARISON_FAILED\", \"verified\": false, \"confidence\": {{randomDecimal lower=0.001 upper=0.499}} }}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}