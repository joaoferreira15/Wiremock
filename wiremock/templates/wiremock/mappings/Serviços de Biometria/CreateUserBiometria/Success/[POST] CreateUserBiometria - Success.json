{
  "priority": 3,
  "request": {
    "method": "POST",
    "url": "/api/1.1/users",
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.process_id",
          "matches": "^(0[1-9]|[1-9][0-9])$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.mac_address",
          "matches": "^([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.app_token",
          "matches": "^(0[1-9]|[1-9][0-9])$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.reference_id",
          "matches": "^(0[1-9]|[1-9][0-9])$"
        }
      }
    ],
    "headers": {
      "Content-Type": {
        "equalTo": "application/json"
      }
    }
  },
  "response": {
    "transformers": ["response-template"],
    "status": 200,
    "body": "{\"Status\": true, \"Data\": {\"Message\": \"OK!\", \"Id\": {{randomInt lower=1 upper=99}}, \"ProcessId\": {{jsonPath request.body '$.process_id'}}, \"Code\": \"200\", \"Message\": \"User created successfully.\"}",
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
